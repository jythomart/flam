pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
clock = {}

player = {}
player.x = 20
player.y = 20
player.vx = 0
player.vy = 0
player.sprite = 1
player.speed = 8
player.flipx = false
player.state = "moving"

coll_idx = {}
coll_idx.first = 128
coll_idx.last = 196

weapon_idx = {}
weapon_idx.first = 12
weapon_idx.last = 42

enemies_idx = {}
enemies_idx.first = 12
enemies_idx.last = 42

function attack()
end

function use_item()
end

function game_input()
	player.vx = 0;
	player.vy = 0;
	if btn(0) then
		player.vx = -player.speed
		player.flipx = true
	end
	if btn(1) then
		player.vx = player.speed
		player.flipx = false
	end
	if btn(2) then
		player.vy = -player.speed
	end
	if btn(3) then
		player.vy = player.speed
	end
	if btn(4) then
		attack()
	end
	if btn(5) then
		use_item()
	end
end

function physic()
	player.x += player.vx * player.speed * clock.delta
	player.y += player.vy * player.speed * clock.delta
end

function _init()
	clock.prev = time()
end

function _update()
	clock.current = time()
	clock.delta = clock.current - clock.prev
	game_input()
	physic()
	clock.prev = clock.current
end

function _draw()
	cls()
	map(0, 0, 0, 0, 16, 16)
	spr(player.sprite, player.x, player.y, 1, 1, player.flipx)
end
